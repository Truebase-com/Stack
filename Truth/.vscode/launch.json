{
	"version": "0.2.0",
	"configurations": [{
		"type": "node",
		"request": "launch",
		"name": "Debug Project",
		"program": "${workspaceFolder}/build/truth.quick.js", 
		"internalConsoleOptions": "neverOpen",
		"sourceMaps": true,
		"args": [
			"--file=./test/baselines/PatternAliasInheritance.truth",
			"--inspect=SubSubType/Thing/Value"
		]
	},
	{
		"name": "Debug Compiler Remotely",
		"type": "chrome",
		"request": "attach",
		"port": 9223,
		"webRoot": "${workspaceRoot}",
		"timeout": 30000,
		"smartStep": true,
		"sourceMaps": true,
		"sourceMapPathOverrides": {
			"**/node_modules/truth-compiler/*": "${webRoot}/*"
		}
	},
	{
		"type": "node",
		"request": "launch",
		"name": "Debug Jest Tests",
		"program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
		"args": [
			
		],
		"internalConsoleOptions": "openOnSessionStart"
	},
	{
		"type": "node",
		"request": "launch",
		"name": "Debug Makets Script",
		"program": "${env:HOME}/.npm-packages/bin/makets",
		"args": [
			"bundle"
		]
	}]
}
